{
  "name": "adp-react",
  "version": "1.0.0",
  "description": "React project for RED Academy ADP",
  "license": "MIT",
  "author": "MK <mackenzie@redacademy.com> (http://mackenziekieran.com)",
  "contributors": [
    "Fergus MacConnell <f.macconnell@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": " https://github.com/redacademy/adp-react.git"
  },
  "scripts": {
    "start": "webpack-validator .webpack/webpack.dev.client.config.js && concurrently 'npm run watch' 'cross-env NODE_ENV=development nodemon --debug --inspect --harmony server' 'npm run browsersync'",
    "browsersync": "browser-sync start --config .webpack/browsersync.js",
    "watch": "webpack --watch --config '.webpack/webpack.dev.client.config.js'",
    "server": "node server/index.js",
    "test": "jest"
  },
  "babel": {
    "env": {
      "production": {
        "only": [
          "web.browser",
          "shared"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    },
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "dependencies": {
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "colors": "^1.1.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "material-ui": "^16.0.0",
    "moment": "^2.15.1",
    "nconf": "^0.8.4",
    "passport": "^0.3.2",
    "passport-jwt": "^2.1.0",
    "passport-local": "^1.0.0",
    "react": "^15.3.0",
    "react-dom": "15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "1.0.0",
    "redux": "^3.6.0",
    "redux-form-material-ui": "^4.1.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "sequelize": "^3.24.5",
    "socket.io": "^1.5.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "max-len": 0,
      "global-require": 0,
      "no-case-declarations": 0,
      "no-param-reassign": 1,
      "prefer-stateless-function": 0,
      "react/jsx-no-bind": 1
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.7",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-preset-es2015-webpack": "6.4.1",
    "babel-preset-jest": "^16.0.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "^6.9.0",
    "browser-sync": "^2.13.0",
    "cross-env": "2.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.5.1",
    "enzyme-to-json": "^1.3.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "1.4.1",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "5.2.2",
    "eventsource-polyfill": "0.9.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.33.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "jest": "^17.0.0",
    "jest-css-modules": "^1.1.0",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.4.1",
    "react-addons-test-utils": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "run-sequence": "^1.2.2",
    "style-loader": "^0.13.1",
    "webpack": "2.1.0-beta.13",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1",
    "webpack-validator": "^2.2.4"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!webpack.config.js"
    ],
    "coverageDirectory": "__coverage__",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
