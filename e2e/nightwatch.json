{
  "src_folders": [
    "e2e"
  ],
  "output_folder": "reports",
  "custom_commands_path": "",
  "custom_assertions_path": "",
  "page_objects_path": "e2e/pages/",
  "globals_path": "",
  "selenium": {
    "start_process": false,
    "server_path": "",
    "log_path": "",
    "port": 4444,
    "cli_args": {
      "webdriver.chrome.driver": "",
      "webdriver.firefox.driver": "/applications/firefox.app/contents/macros/firefox-bin",
      "webdriver.gecko.driver": "",
      "webdriver.safari.driver": "",
      "webdriver.edge.driver": ""
    }
  },
  "test_settings": {
    "default": {
      "launch_url": "http://localhost",
      "selenium_port": 4444,
      "selenium_host": "localhost",
      "silent": true,
      "screenshots": {
        "enabled": true,
        "on_failure": true,
        "on_error": false,
        "path": "e2e/screenshots/fail"
      },
      "desiredCapabilities": {
        "browserName": "chrome",
        "marionette": true
      }
    },
    "chrome": {
      "desiredCapabilities": {
        "browserName": "chrome"
      },
      "firefox": {
        "desiredCapabilities": {
          "browserName": "firefox",
          "javascriptEnabled": true,
          "acceptSslCerts": true
        }
      }
    }
  }
}